    "body-parser": "^1.17.2",
    "connect-redis": "^3.3.0",
    "express": "^4.14.0",
    "express-minify-html": "^0.6.0",
    "express-session": "^1.15.4",
    "gulp": "^3.9.1",
    "gulp-clean-css": "^3.0.4",
    "gulp-less": "^3.3.0",
    "gulp-sourcemaps": "^2.6.0",
    "gulp-uglify": "^2.1.2",
    "htmling": "0.0.7",
    "request": "^2.78.0"
  }
}
