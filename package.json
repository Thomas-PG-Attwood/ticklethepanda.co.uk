  "description": "",
  "scripts": {
    "git": "git checkout master && git fetch && git reset origin/master --hard",
    "update-dependancies": "npm install",
    "less": "lessc _less/style.css public/default.css",
    "compress-images": "find ./public/images -name \"*.jpg\" -type f -exec jpegtran -copy none -optimize -outfile {} {} \\;",
    "start": "npm run git && npm run update-dependancies && npm run compress-images && npm run less"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ticklethepanda/ticklethepanda.co.uk.git"
  },
  "author": "",
  "dependencies": {
    "less": "^2.7.1"
  }
}
